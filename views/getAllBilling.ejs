<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB - getAllBilling</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="container">
    <h2>Database Results Getting Billing</h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li>ID: <%= r._id %></li>
        <ul>
            <li>Customer ID: <%= r.CUSTOMER_ID %></li>
            <li>Credit Card Type: <%=r.CREDITCARDTYPE %></li>
            <li>Credit Card Number: <%=r.CREDITCARDNUM %></li>
            <li>Credit Card Expiration Date: <%=r.CREDITCARDEXP %></li>
            <li>Credit Card Security Number: <%=r.CREDITCARDSECURITYNUM %></li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>